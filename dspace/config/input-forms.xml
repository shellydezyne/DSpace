         <dc-element>contributor</dc-element>
         <dc-qualifier>coordinator</dc-qualifier>
         <repeatable>true</repeatable>
         <label>Coordinator(s)</label>
         <input-type>name</input-type>
         <hint>Enter the names of the coordinators</hint>
         <required>You must enter a main coordinator for this item.</required>
       </field>
       <field>
         <dc-schema>dc</dc-schema>
         <dc-element>contributor</dc-element>
         <dc-qualifier>author</dc-qualifier>
